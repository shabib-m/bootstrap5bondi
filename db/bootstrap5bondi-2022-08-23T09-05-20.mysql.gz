INSERT INTO `locales_source` VALUES ('1','/core/install.php?langcode=ar&profile=standard','Anonymous','config:system.core:anonymous','1.22.2'),('2','/core/install.php?langcode=ar&profile=standard','Backdrop CMS','config:system.core:site_name','1.22.2'),('3','/core/install.php?langcode=ar&profile=standard','','config:system.core:site_slogan','1.22.2'),('4','/core/install.php?langcode=ar&profile=standard','[site:name] is currently under maintenance. We should be back shortly. Thank you for your patience.','config:system.core:maintenance_mode_message','1.22.2'),('5','/core/install.php?langcode=ar&profile=standard','[user:name],\n\nA request to cancel your account has been made at [site:name].\n\nYou may now cancel your account on [site:url:brief] by clicking this link or copying and pasting it into your browser:\n\n[user:cancel-url]\n\nNOTE: The cancellation of your account is not reversible.\n\nThis link expires in one day and nothing will happen if it is not used.\n\n--  [site:name] team','config:user.mail:cancel_confirm_body','1.22.2'),('6','/core/install.php?langcode=ar&profile=standard','Account cancellation request for [user:name] at [site:name]','config:user.mail:cancel_confirm_subject','1.22.2'),('7','/core/install.php?langcode=ar&profile=standard','[user:name],\n\nA request to reset the password for your account has been made at [site:name].\n\nYou may now reset your password by clicking this link or copying and pasting it to your browser:\n\n[user:one-time-login-url]\n\nThis link can only be used once and will lead you to a page where you can set your password. It expires after one day and nothing will happen if it is not used.\n\n--  [site:name] team','config:user.mail:password_reset_body','1.22.2'),('8','/core/install.php?langcode=ar&profile=standard','Password reset information for [user:name] at [site:name]','config:user.mail:password_reset_subject','1.22.2'),('9','/core/install.php?langcode=ar&profile=standard','[user:name],\n\nA site administrator at [site:name] has created an account for you. You may now log in by clicking this link or copying and pasting it to your browser:\n\n[user:one-time-login-url]\n\nThis link can only be used once to log in and will lead you to a page where you can set your password.\n\nAfter setting your password, you will be able to log in at [site:login-url] in the future using:\n\nusername: [user:name]\npassword: Your password\n\n--  [site:name] team','config:user.mail:register_admin_created_body','1.22.2');
INSERT INTO `locales_source` VALUES ('10','/core/install.php?langcode=ar&profile=standard','An administrator created an account for you at [site:name]','config:user.mail:register_admin_created_subject','1.22.2'),('11','/core/install.php?langcode=ar&profile=standard','[user:name],\n\nThank you for registering at [site:name]. You may now log in by clicking this link or copying and pasting it to your browser:\n\n[user:one-time-login-url]\n\nThis link can only be used once to log in and will lead you to a page where you can set your password.\n\nAfter setting your password, you will be able to log in at [site:login-url] in the future using:\n\nusername: [user:name]\npassword: Your password\n\n--  [site:name] team','config:user.mail:register_no_approval_required_body','1.22.2'),('12','/core/install.php?langcode=ar&profile=standard','Account details for [user:name] at [site:name]','config:user.mail:register_no_approval_required_subject','1.22.2'),('13','/core/install.php?langcode=ar&profile=standard','[user:name],\n\nThank you for registering at [site:name]. Your application for an account is currently pending approval. Once it has been approved, you will receive another e-mail containing information about how to log in, set your password, and other details.\n\n\n--  [site:name] team','config:user.mail:register_pending_approval_body','1.22.2'),('14','/core/install.php?langcode=ar&profile=standard','Account details for [user:name] at [site:name] (pending admin approval)','config:user.mail:register_pending_approval_subject','1.22.2'),('15','/core/install.php?langcode=ar&profile=standard','[user:name] has applied for an account.\n\n[user:edit-url]','config:user.mail:register_pending_approval_admin_body','1.22.2'),('16','/core/install.php?langcode=ar&profile=standard','New account of [user:name] at [site:name] (pending approval)','config:user.mail:register_pending_approval_admin_subject','1.22.2'),('17','/core/install.php?langcode=ar&profile=standard','[user:name],\n\nYour account at [site:name] has been activated.\n\nYou may now log in by clicking this link or copying and pasting it into your browser:\n\n[user:one-time-login-url]\n\nThis link can only be used once to log in and will lead you to a page where you can set your password.\n\nAfter setting your password, you will be able to log in at [site:login-url] in the future using:\n\nusername: [user:name]\npassword: Your password\n\n--  [site:name] team','config:user.mail:status_activated_body','1.22.2');
